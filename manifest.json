{
  "name": "CollabCoding",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Collaborative code editor for HTML5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CollabCoding"
  },
  "conent_scripts": [
    { "js": ["assets/scripts/chronoline/raphael-min.js"] }
  ],
  "permissions": [
    "tabs", "history", "browsingData", "bookmarks", "chrome://favicon/*"
  ]
}
